{
    "__python_version": "3.12",
    "__uv_version": "0.10.2",
    "__pre_commit_version": "4.5",
    "__ruff_version": "0.15.0",
    "__pytest_version": "9.0",
    "__notebook_version": "7.5",
    "project_name": "Project",
    "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '-') }}",
    "module_name": "{{ cookiecutter.project_slug.replace('-', '_') }}",
    "use_ruff_format": true,
    "use_ruff_lint": true,
    "use_pytest": true,
    "use_uv": true,
    "__prompts__": {
        "project_name": "Project name, Title Case",
        "project_slug": "Repository name, kebab-case",
        "module_name": "Python module name, snake_case",
        "use_ruff_format": "Enable code formatting, using Ruff",
        "use_ruff_lint": "Enable code linting, using Ruff",
        "use_pytest": "Enable unit testing, using pytest",
        "use_uv": "Use uv instead of pip"
    }
}
